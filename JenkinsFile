pipeline {
    agent any

    tools {
        maven 'apache-maven-3.6.3'
        mavenSettingsConfig: 'MyGlobalSettings'
    }

    stages {
        stage('build') {
            steps {
              sh "mvn -version"
              sh "mvn clean install"
            }
        }
    }

    post {
        always {
            cleanWs()
        }
    }
}