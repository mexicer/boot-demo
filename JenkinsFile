pipeline {
    agent any
    stages {
        stage('build') {
        steps{
             withMaven(
                    maven: 'local_mvn',
                    mavenSettingsConfig: 'MyGlobalSettings') {
                       sh "mvn clean install"
                }
             }
        }
    }

    post {
        always {
            cleanWs()
        }
    }
}