pipeline {
    agent {
        docker{
            image 'maven:3.6.0'
            args '-v /home/ubuntu/Web/maven/.m2:/var/jenkins_home/downloads/.m2'
        }
    }
    
    stages {
        stage('build') {
            steps {
              sh "mvn -version"
              sh "mvn clean install -U"
            }
        }
    }

}